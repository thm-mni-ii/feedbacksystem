{
  "problemMatcher": [
    {
      "owner": "pylint",
      "pattern": [
        {
          "regexp": "^(.*):(\\d+):(\\d+):\\s([EF]\\d+):\\s(.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "code": 4,
          "message": 5
        }
      ]
    },
    {
      "owner": "pylint-warning",
      "severity": "warning",
      "pattern": [
        {
          "regexp": "^(.*):(\\d+):(\\d+):\\s([CRW]\\d+):\\s(.*)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "code": 4,
          "message": 5
        }
      ]
    },
    {
      "owner": "black",
      "severity": "warning",
      "pattern": [
        {
          "regexp": "^(would reformat\\s(.*))$",
          "message": 1,
          "file": 2
        }
      ]
    }
  ]
}
