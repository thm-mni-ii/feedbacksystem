{
  "problemMatcher": [
    {
      "owner": "pylint",
      "pattern": [
        {
          "regexp": "^(.*):(\\d+):(\\d+):\\s([EF]\\d+):\\s([.\\s\\S]*?)\\s\\((.*)\\)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "message": 4,
          "code": 5
        }
      ]
    },
    {
      "owner": "pylint-warning",
      "severity": "warning",
      "pattern": [
        {
          "regexp": "^(.*):(\\d+):(\\d+):\\s([CRW]\\d+):\\s([.\\s\\S]*?)\\s\\((.*)\\)$",
          "file": 1,
          "line": 2,
          "column": 3,
          "message": 4,
          "code": 5
        }
      ]
    },
    {
      "owner": "black",
      "severity": "warning",
      "pattern": [
        {
          "regexp": "^(--- .*.py\\s+[0-9]{4}-[0-9]{2}-[0-9]{2} [0-9]{2}:[0-9]{2}:[0-9]{2}.[0-9]{6} \\+[0]{4}[.\\s\\S]*?would reformat (.*))$",
          "message": 1,
          "file": 2
        }
      ]
    }
  ]
}
